{
    "name": "spipu/symfony-dev",
    "type": "project",
    "license": "MIT",
    "description": "A project to develop bundles",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "*",
        "sensiolabs/security-checker": "*",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "*",
        "symfony/twig-pack": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "twbs/bootstrap": "^4.0"
    },
    "require-dev": {
        "edgedesign/phpqa": "^1.23",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "pdepend/pdepend": "^2.5",
        "phpmd/phpmd": "^2.7",
        "phpmetrics/phpmetrics": "^2.4",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "*",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/profiler-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.2.24",
            "ext-bcmath": "1",
            "ext-ctype": "1",
            "ext-gd": "1",
            "ext-spl": "1",
            "ext-dom": "1",
            "ext-simplexml": "1",
            "ext-mcrypt": "1",
            "ext-hash": "1",
            "ext-curl": "1",
            "ext-iconv": "1",
            "ext-intl": "1",
            "ext-xsl": "1",
            "ext-mbstring": "1",
            "ext-openssl": "1",
            "ext-zip": "1",
            "ext-pdo_mysql": "1",
            "ext-soap": "1",
            "lib-libxml": "1"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Spipu\\": "src/Spipu/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*",
        "spipu/ui-bundle": "*",
        "spipu/configuration-bundle": "*",
        "spipu/user-bundle": "*",
        "spipu/process-bundle": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "spipu:assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "v4.4.*"
        }
    }
}
